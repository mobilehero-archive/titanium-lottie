{
	"name": "@titanium/lottie",
	"version": "1.0.49",
	"description": "‚≠ê  Axway Amplify modules to use Airbnb Lottie animations with Appcelerator Titanium SDK",
	"main": "index.js",
	"scripts": {
		"postinstall": "npx @titanium/tiapp-xml install",
		"postuninstall": "npx @titanium/tiapp-xml uninstall",
		"lint": "eslint . --fix",
		"list": "npm pack && tar -xvzf *.tgz && rm -rf package *.tgz",
		"prep": "npm install --ignore-scripts",
		"test": "npm run prep",
		"prepublishOnly": "git pull && npm test && bump insane",
		"prepublishOnly2": "git pull && npm test && npm audit fix && bump insane"
	},
	"maintainers": [
		{
			"name": "Brenton House"
		}
	],
	"repository": {
		"type": "git",
		"url": "github:mobilehero-archive/titanium-lottie"
	},
	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/mobilehero-archive/titanium-lottie/issues"
	},
	"homepage": "https://brenton.house/saying-goodbye-to-axway-amplify-titanium-31a44f3671de",
	"keywords": [
		"titanium",
		"mobile",
		"alloy",
		"turbo",
		"axway",
		"appcelerator",
		"ios",
		"android",
		"iphone",
		"ipad",
		"lottie",
		"animation",
		"titanium-native-module",
		"plugin",
		"amplify",
		"api-builder",
		"appcelerator-titanium",
		"titanium-sdk",
		"titanium-mobile",
		"titanium-module",
		"axway-sdk",
		"axway-amplify",
		"brenton-house"
	],
	"titanium": {
		"type": "native-module",
		"platform": {
			"ios": {
				"moduleid": "ti.animation",
				"name": "ti.animation",
				"version": "2.0.0",
				"apiversion": 2,
				"architectures": [
					"armv7",
					"arm64",
					"i386",
					"x86_64"
				],
				"minsdk": "8.1.0",
				"guid": "1fcfea69-bbbf-42f8-b738-9545b5dda74d"
			},
			"android": {
				"moduleid": "ti.animation",
				"name": "ti.animation",
				"version": "4.0.0",
				"apiversion": 4,
				"architectures": [
					"arm64-v8a",
					"armeabi-v7a",
					"x86",
					"x86_64"
				],
				"minsdk": "9.0.0",
				"guid": "0af214dd-5fa0-4cc7-b8c3-268fb78a03d5"
			}
		}
	},
	"developerDependencies": {
		"lodash": "^4.17.15"
	},
	"devDependencies": {
		"@geek/eslint-config": "^1.0.3",
		"@titanium/eslint-config": "^1.0.25",
		"eslint": "^8.6.0"
	},
	"dependencies": {
		"@geek/cache": "^1.0.2",
		"@geek/config": "^0.2.4",
		"@geek/dotenv": "^0.0.9",
		"@geek/jsonc": "^1.0.4",
		"@geek/jwt": "^1.0.1",
		"@geek/logger": "^1.0.18",
		"@geek/retry": "^1.0.0",
		"@geek/spawn": "^1.0.2",
		"@geek/spinner": "^1.0.2",
		"@geek/tld": "^2.0.1-1",
		"@titanium/appcelerator": "^6.0.21",
		"@titanium/bluebird": "^3.7.26",
		"@titanium/errors": "^1.0.21",
		"@titanium/essentials": "^1.0.55",
